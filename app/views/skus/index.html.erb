<p id="notice"><%= notice %></p>

<h1>Listing Skus</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Quantity</th>
      <th>Color</th>
      <th>Weight</th>
      <th>Width</th>
      <th>Height</th>
      <th>Material</th>
      <th>Attributes</th>
      <th>Active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @skus.each do |sku| %>
      <tr>
        <td><%= sku.name %></td>
        <td><%= sku.description %></td>
        <td><%= sku.quantity %></td>
        <td><%= sku.color %></td>
        <td><%= sku.weight %></td>
        <td><%= sku.width %></td>
        <td><%= sku.height %></td>
        <td><%= sku.material %></td>
        <td><%= sku.attributes %></td>
        <td><%= sku.active %></td>
        <td><%= link_to 'Show', sku %></td>
        <td><%= link_to 'Edit', edit_sku_path(sku) %></td>
        <td><%= link_to 'Destroy', sku, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sku', new_sku_path %>
